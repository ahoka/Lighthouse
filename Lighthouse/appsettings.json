{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Raft": {
    "Address": "http://0.0.0.0:9000",
    "Join": [ "http://127.0.0.1:9001", "http://127.0.0.1:9002" ]
  },
  "Persistence": {
    "DataDirectory":  "."
  }
}
